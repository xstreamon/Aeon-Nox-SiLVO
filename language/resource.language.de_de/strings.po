msgstr "Audio-Erweiterungen"

msgctxt "#31802"
msgid "Video addons / Audio addons"
msgstr "Video-/Audio-Erweiterungen"

msgctxt "#31803"
msgid "Program addons"
msgstr "Programmerweiterungen"

msgctxt "#31804"
